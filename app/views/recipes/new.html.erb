<%= form_for @recipe do |f| %>
    Title: <%= f.text_field :title %><br>
  
    <%= f.fields_for :ingredients do |ingr| %>
      <%= ingr.label :name, "Ingredient name: " %>
      <%= ingr.text_field :name %><br>
  
      <%= ingr.label :quantity, "Ingredient quantity: " %>
      <%= ingr.text_field :quantity %><br>
    <% end %>
    <%= f.submit :"save recipe" %>
  
  <% end %>